    "keywords": ["yii2","doctrine"],
    "homepage": "https://github.com/liketomove/yii2-doctrine",
    "type": "yii-extension",
    "license": "BSD-3-Clause",
    "support": {
        "source": "https://github.com/liketomove/yii2-doctrine"
    },
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Gipi Scioni",
            "email": "team@beemyweb.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "doctrine/orm": "2.6.*"
    },
    "autoload": {
        "psr-4": {
            "yii\\doctrine\\": ""
        }
    }
}
